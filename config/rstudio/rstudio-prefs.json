{
    "show_margin": false,
    "default_project_location": "~/dev",
    "pdf_previewer": "none",
    "rmd_chunk_output_inline": false,
    "wrap_tab_navigation": false,
    "save_workspace": "never",
    "reuse_sessions_for_project_links": true,
    "svn_exe_path": "/home/jvfe",
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true
    },
    "load_workspace": false,
    "remove_history_duplicates": true,
    "editor_theme": "Material Darker {rsthemes}",
    "posix_terminal_shell": "zsh",
    "jobs_tab_visibility": "shown",
    "document_author": "Jo√£o Vitor F Cavalcante",
    "rmd_viewer_type": "pane",
    "spelling_dictionary_language": "en_AU",
    "real_time_spellchecking": false,
    "latex_preview_on_cursor_idle": "never",
    "always_save_history": false,
    "use_tinytex": true,
    "initial_working_directory": "~/dev",
    "editor_keybindings": "vim",
    "rainbow_parentheses": true,
    "source_with_echo": true,
    "strip_trailing_whitespace": true,
    "auto_expand_error_tracebacks": true,
    "warn_variable_defined_but_not_used": true,
    "default_encoding": "UTF-8"
